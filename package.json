{
	"name": "acm-ai-website",
	"version": "0.1.0",
	"prepare": "husky install",
	"private": true,
	"lint-staged": {
		"*.{js,jsx,ts,tsx}": "eslint --fix ",
		"*.{ts,tsx,js,jsx,css,md,html,json}": "prettier --cache --write"
	},
	"scripts": {
		"dev": "next dev",
		"build": "next build",
		"start": "next start",
		"get_assets": "bash $PWD/scripts/getAssets.sh",
		"lint": "eslint . --fix --max-warnings=0",
		"format": "prettier . --write",
		"postinstall": "husky install"
	},
	"dependencies": {
		"next": "14.0.4",
		"react": "^18",
		"react-dom": "^18"
	},
	"devDependencies": {
		"@types/eslint": "^8.56.0",
		"@types/node": "^20",
		"@types/prettier": "^3.0.0",
		"@types/react": "^18.2.45",
		"@types/react-dom": "^18",
		"@types/sass": "^1.45.0",
		"@typescript-eslint/eslint-plugin": "^6.15.0",
		"@typescript-eslint/parser": "^6.15.0",
		"eslint": "8.56.0",
		"eslint-config-next": "14.0.4",
		"eslint-config-prettier": "9.1.0",
		"eslint-plugin-prettier": "5.1.1",
		"husky": "^8.0.0",
		"lint-staged": "15.2.0",
		"prettier": "3.1.1",
		"sass": "1.69.5",
		"typescript": "5.3.3"
	},
	"packageManager": "yarn@4.0.2"
}
